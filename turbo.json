{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "cacheDir": ".turbo/cache",
  "tasks": {
    "build": {
      "dependsOn": ["^check-types", "^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["**/build/**", "**/dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "with": ["db:studio"]
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    },
    "db:migrate": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false,
      "persistent": true
    }
  }
}
